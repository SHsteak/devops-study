pipeline {
    agent {
        kubernetes {
        yamlFile 'jenkins/pod_template/test.yaml'
        }
    }
    stages {
        stage('Test') {
            parallel {
                stage('itsm') {
                    steps {
                        container('amazoncorretto-16') {
                            dir('itsm') {
                                sh 'pwd'
                            }
                        }
                    }
                }
                stage('ubuntu') {
                    steps {
                        container('ubuntu') {
                            sh '''
                                pwd
                                ls -al $HOME
                            '''
                        }
                    }
                }
            }
        }
    }
}